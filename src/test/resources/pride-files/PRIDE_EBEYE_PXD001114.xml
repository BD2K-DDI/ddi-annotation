    <entry id="PXD001114">
      <name>Characterization of LysargiNase for use in phosphoproteomics experiments, partII</name>
      <description>Use of parallel digest with LysargiNase and trypsin to cover complementary phosphosites / characterization of phospho-motifs preferentially identified by each protease</description>
      <cross_references>
        <ref dbkey="9606" dbname="TAXONOMY"/>
        <ref dbkey="25419962" dbname="pubmed"/>
      </cross_references>
