    <entry id="PXD001378">
      <name>Characterization of LysargiNase peptide fragmentation</name>
      <description>Comparison of Lyarginase and tryptic digests with regard to fragmentation behaviour/effect of inclusing a ion in peptide assignment scoring</description>
      <cross_references>
        <ref dbkey="562" dbname="TAXONOMY"/>
        <ref dbkey="25419962" dbname="pubmed"/>
      </cross_references>
      <dates>
