    <entry id="PXD002210">
      <name>iTRAQ of NTHi phasevarions</name>
      <description>iTRAQ quantitative mass spec of proteins differentially regulated by biphasic switching of modA methyltransferase alleles in the human pathogen non-typeable Haemophilus influenzae</description>
      <cross_references>
        <ref dbkey="727" dbname="TAXONOMY"/>
      </cross_references>
      <dates>
        <date type="submission" value="2015-05-20"/>
